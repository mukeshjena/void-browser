# Auto detect text files and perform LF normalization
* text=auto

# Explicitly declare text files you want to always be normalized and converted
# to native line endings on checkout.
*.dart text eol=lf
*.md text eol=lf
*.yaml text eol=lf
*.yml text eol=lf
*.json text eol=lf
*.txt text eol=lf
*.xml text eol=lf
*.gradle text eol=lf
*.properties text eol=lf
*.kt text eol=lf
*.java text eol=lf
*.swift text eol=lf
*.m text eol=lf
*.h text eol=lf
*.cpp text eol=lf
*.cc text eol=lf
*.c text eol=lf
*.hpp text eol=lf
*.cmake text eol=lf
*.sh text eol=lf
*.bat text eol=crlf
*.ps1 text eol=crlf

# Denote all files that are truly binary and should not be modified.
*.png binary
*.jpg binary
*.jpeg binary
*.gif binary
*.ico binary
*.webp binary
*.svg binary
*.woff binary
*.woff2 binary
*.ttf binary
*.otf binary
*.eot binary
*.jar binary
*.class binary
*.so binary
*.dylib binary
*.dll binary
*.exe binary
*.apk binary
*.aab binary
*.aar binary
*.zip binary
*.tar binary
*.gz binary

# Gradle wrapper
gradle-wrapper.jar binary

